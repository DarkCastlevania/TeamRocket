@page
@model IndexModel
@{
    ViewData["Title"] = "Equipo Rocket Enterprise";
}

<div class="text-center">
    <h1 class="display-4">Estamos Preparados</h1>
    <p>... <a href="https://docs.microsoft.com/aspnet/core">Para los problemas... y mas vale que teman.</a>.</p>
</div>
@if (User.Identity.IsAuthenticated)
{
    <table class="table table-sm">
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
}